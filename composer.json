{
    "name": "nvkode/nvdoc",
    "description": "Documentation generator for Symfony projects.",
    "type": "library",
    "license": "MIT",
    "version": "0.0.3",
    "autoload": {
        "psr-4": {
            "Nvkode\\Nvdoc\\": "src/",
            "Nvkode\\Nvdoc\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Mykyta Melnyk",
            "email": "liswelus@gmail.com"
        }
    ],
    "scripts": {
        "test": "phpunit tests",
        "validation:code": [
            "phpcs src tests",
            "phpstan analyse src tests"
        ]
    },
    "minimum-stability": "stable",
    "require-dev": {
        "phpunit/phpunit": "^10.0",
        "squizlabs/php_codesniffer": "3.7.2",
        "phpstan/phpstan": "^1.10"
    },
    "require": {
        "symfony/finder": "^6.2",
        "php": ">= 8.1"
    }
}
